{
    // IntelliSense를 사용하여 가능한 특성에 대해 알아보세요.
    // 기존 특성에 대한 설명을 보려면 가리킵니다.
    // 자세한 내용을 보려면 https://go.microsoft.com/fwlink/?linkid=830387을(를) 방문하세요.
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Java",
            "type": "java",
            "request": "launch",
            "stopOnEntry": true,
            "jdkPath": "${env:JAVA_HOME}/bin",
            "cwd": "${fileDirname}",
            "startupClass": "${fileBasenameNoExtension}",
            "classpath": [
                ".",
                "${fileDirname}"
            ]
        },
        {
            "name": "Java Console App",
            "type": "java",
            "request": "launch",
            "stopOnEntry": true,
            "jdkPath": "${env:JAVA_HOME}/bin",
            "cwd": "${fileDirname}",
            "startupClass": "${fileBasenameNoExtension}",
            "classpath": [
                ".",
                "${fileDirname}"
            ],
            "externalConsole": true
        },
        {
            "type": "java",
            "name": "Debug (Launch) - Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "type": "java",
            "name": "Debug (Launch)-Hobbies<FavoriteCoffeeAppServer>",
            "request": "launch",
            "mainClass": "Hobbies",
            "projectName": "FavoriteCoffeeAppServer"
        },
        {
            "type": "java",
            "name": "Debug (Launch)-FavoriteCoffeeAppServerApplication<FavoriteCoffeeAppServer>",
            "request": "launch",
            "mainClass": "com.coffee.FavoriteCoffeeAppServerApplication",
            "projectName": "FavoriteCoffeeAppServer"
        },
        {
            "type": "java",
            "name": "Debug (Launch)-testMain<FavoriteCoffeeAppServer>",
            "request": "launch",
            "mainClass": "com.coffee.service.testMain",
            "projectName": "FavoriteCoffeeAppServer"
        }
    ]
}